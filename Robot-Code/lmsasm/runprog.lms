vmthread MAIN
{
  DATA32 Timer

  UI_DRAW(FILLWINDOW,0x00,0,0)
  UI_DRAW(TEXT,FG_COLOR,10,50,'Back to Exit')
  UI_DRAW(UPDATE)

  TIMER_WAIT(200, Timer)
  TIMER_READY(Timer)

	DATA32 Status
	SYSTEM('/media/card/standalone',Status)

CheckBackButton:

  DATA8 BackPressed
  UI_BUTTON(PRESSED, BACK_BUTTON, BackPressed)

  JR_FALSE(BackPressed, CheckBackButton)
}
