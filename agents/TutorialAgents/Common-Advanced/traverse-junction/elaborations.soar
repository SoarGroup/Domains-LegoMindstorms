
sp {traverse-junction*elaboration*elaborate-relative-direction
	(state <s>	^name traverse-junction
				^superstate <ss>)
	(<ss>	^direction-map.direction <d>
			^selected-direction <selected-direction>
			^current-direction <current-direction>)
	(<d>	^name <current-direction>
			^<relative> <selected>)
	(<selected>	^name <selected-direction>)
-->
	(<s>	^relative-direction <relative>)
}

sp {traverse-junction*elaboration*elaborate-color-sensor-values
	(state <s>	^name traverse-junction
				^superstate <ss>)
	(<ss>	^color-sensor-values <vals>)
-->
	(<s>	^color-sensor-values <vals>)
}

sp {traverse-junction*elaboration*elaborate-motor-ports
	(state <s>	^name traverse-junction
				^superstate <ss>)
	(<ss>	^left-motor-port <left-port>
			^right-motor-port <right-port>
			^io <io>)
-->
	(<s>	^left-motor-port <left-port>
			^right-motor-port <right-port>
			^io <io>)
}
