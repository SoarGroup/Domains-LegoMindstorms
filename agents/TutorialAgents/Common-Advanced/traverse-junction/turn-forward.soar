
sp {traverse-junctions*propose*turn*forward
	(state <s>	^name traverse-junction
				^superstate <ss>
				^relative-direction forward)
	(<ss>	^has-centered
			-^turned)
-->
	(<s>	^operator <o>)
	(<o>	^name turn
			^action stop)
}

sp {traverse-junction*apply*turn*forward*rentrant-support
	(state <s>	^name traverse-junction
				^operator.name turn
				^relative-direction forward
				^superstate <ss>)
	(<ss>		^operator.name traverse-junction
				^has-centered)
-->
	(<ss>	^turned true)
}
