
sp {traverse-junction*propose*move-off-junction*non-junction-color
	(state <s>	^name traverse-junction
				^superstate <ss>
				^color-sensor-values <values>)
	(<ss>	^turned)
	(<values>	^left black
				^center black
				^right black)
-->
	(<s>	^operator <o>)
	(<o>	^name move-forward
			^non-junction-color true)
}

sp {traverse-junction*propose*move-off-junction*junction-color
	(state <s>	^name traverse-junction
				^superstate <ss>
				^color-sensor-values <values>)
	(<ss>	^turned
			^junction-color <color>
			-^moved-off-junction)
	(<values>	^left <color>
				^right <color>
				^center black)
-->
	(<s>	^operator <o>)
	(<o>	^name move-forward)
}

sp {traverse-junction*apply*move-off-junction*juntion-color
	(state <s>	^operator <o>
				^superstate <ss>)
	(<o>	^name move-forward
			^<< junction-color non-junction-color >> true)
	(<ss>	^operator.name traverse-junction)
-->
	(<ss>	^moved-off-junction-almost true)
}
