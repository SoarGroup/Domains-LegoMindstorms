
sp {traverse-junction*propose*move-to-center-of-junction
	(state <s>	^name traverse-junction
				-^superstate.has-centered)
-->
	(<s>	^operator <o> +)
	(<o>	^name move-forward
			^centering true)
}

sp {traverse-junction*apply*move-to-center*has-centered
	(state <s>	^operator <o>
				^superstate <ss>
				^color-sensor-values <values>
				^io.output-link <out>)
	(<o>	^name move-forward
			^centering true)
	(<ss>	^operator.name traverse-junction)
	(<values>	^left black
				^right black
				^center black)
-->
	(<ss>	^has-centered true)
}
