
sp {handle-junction*apply*move-forward*send-commands
	(state <s>	^name handle-junction
				^operator.name move-forward
				^io.output-link <out>
				^left-motor-port <left-port>
				^right-motor-port <right-port>)
-->
	# Create a command for each motor on the output
	# link
	(<out>	^motor <left>
			^motor <right>)
	(<left>		^port <left-port>
				^set-direction backward
				^set-power 40)
	(<right>	^port <right-port>
				^set-direction backward
				^set-power 40)
}
