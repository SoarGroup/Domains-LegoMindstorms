
sp {propose*clean-up-handle-junction
	(state <s>	^name line-follower
				^moved-off-junction)
-->
	(<s>	^operator <o> + >)
	(<o>	^name clean-up-handle-junction
			^action stop)
}

sp {apply*clean-up-handle-junction*selected-direction
	(state <s>	^operator.name clean-up-handle-junction
				^selected-direction <v>)
-->
	(<s>	^selected-direction <v> -)
}

