sp {Common-Include*Init-Agent*apply*init-agent
   (state <s> ^operator.name init-agent)
-->
	(<s>	^explorer <s>
			^name Line-Follower
			^mode follow
			^left-motor-port 2
			^right-motor-port 3
			^color-meaning <cm>)
	(<cm>	^junctions <j>)
			^forward 1)
	(<j>	^left 5
			^right 2
			^straight 3
			^end 4)
}
