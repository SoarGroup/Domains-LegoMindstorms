sp {Common-Include*Init-Agent*apply*init-agent
   (state <s> ^operator.name init-agent)
-->
	(<s>	^explorer <s>
			^name Line-Follower
			^mode follow
			^left-motor-port 2
			^right-motor-port 3
			^color-meaning <cm>)
	(<cm>	^junctions <j>)
}

smem --add {
(<s>	^forward 1
		^junctions <j>)
(<j>	^left 5
		^right 2
		^straight 3
		^end 4)
}
