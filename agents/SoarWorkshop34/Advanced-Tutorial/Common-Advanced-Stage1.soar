sp {Common-Advanced*apply*Init-Agent*set*current-direction
   (state <s> ^operator.name init-agent)
-->
   (<s> ^current-direction west)
}

sp {Common-Advanced*apply*Init-Agent*create*color-map
   (state <s> ^operator.name init-agent)
-->
   (<s> ^color-map <map>)
   (<map> ^red west
          ^yellow south
          ^green north
          ^blue east)
}
